{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "plugins": ["import", "react-hooks"],
  "rules": {
    "no-underscore-dangle": "off",
    "no-tabs": "off",
    "no-trailing-spaces": "off",
    "react/forbid-prop-types": 0,
    "object-curly-newline": 0,
    "max-len": ["error",{"code": 200}]
  }
}
